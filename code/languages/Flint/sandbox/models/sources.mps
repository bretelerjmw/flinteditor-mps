                <node concept="19SUe$" id="14WD1prom7p" role="19SJt6">
                  <property role="19SUeA" value="the supervisory authority which is competent pursuant to Article 55 or 56;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prom7q" role="3MKX6F">
