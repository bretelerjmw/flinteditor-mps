                <node concept="19SUe$" id="14WD1prolPD" role="19SJt6">
                  <property role="19SUeA" value="the existence of appropriate safeguards, which may include encryption or pseudonymisation." />
                </node>
              </node>
            </node>
          </node>
        </node>
