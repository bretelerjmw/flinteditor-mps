                <node concept="19SUe$" id="14WD1prolOT" role="19SJt6">
                  <property role="19SUeA" value="processing is necessary in order to protect the vital interests of the data subject or of another natural person;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolOU" role="3MKX6F">
              <property role="3MLT8O" value="(e)" />
              <property role="1hTQn4" value="https://calculemus.org/85c8d6d4-754e-44ed-8899-52adbe4d1ed4" />
