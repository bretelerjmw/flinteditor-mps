          <node concept="19SGf9" id="14WD1prom4F" role="3MLT8Q">
            <node concept="19SUe$" id="14WD1prom4G" role="19SJt6">
              <property role="19SUeA" value="The data protection officer shall be bound by secrecy or confidentiality concerning the performance of his or her tasks, in accordance with Union or Member State law." />
            </node>
          </node>
        </node>
        <node concept="3MKX6G" id="14WD1prom4H" role="3MKX6F">
          <property role="3MLT8O" value="6." />
          <property role="1hTQn4" value="https://calculemus.org/6236b58d-77bb-4606-abc5-5ed4e9527880" />
