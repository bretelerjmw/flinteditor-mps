                <node concept="19SUe$" id="14WD1prolPt" role="19SJt6">
                  <property role="19SUeA" value="any link between the purposes for which the personal data have been collected and the purposes of the intended further processing;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolPu" role="3MKX6F">
              <property role="3MLT8O" value="(b)" />
              <property role="1hTQn4" value="https://calculemus.org/d94e99f1-0e86-4add-9642-ded4bd7958b8" />
              <node concept="19SGf9" id="14WD1prolPv" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prolPw" role="19SJt6">
                  <property role="19SUeA" value="the context in which the personal data have been collected, in particular regarding the relationship between data subjects and the controller;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolPx" role="3MKX6F">
              <property role="3MLT8O" value="(c)" />
              <property role="1hTQn4" value="https://calculemus.org/80ebaa71-04ea-42b2-a5f0-96a9b9b5b3ed" />
              <node concept="19SGf9" id="14WD1prolPy" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prolPz" role="19SJt6">
                  <property role="19SUeA" value="the nature of the personal data, in particular whether special categories of personal data are processed, pursuant to Article 9, or whether personal data related to criminal convictions and offences are processed, pursuant to Article 10;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolP$" role="3MKX6F">
              <property role="3MLT8O" value="(d)" />
              <property role="1hTQn4" value="https://calculemus.org/e040bad5-b7cf-463e-934e-bba1a6d47fd1" />
              <node concept="19SGf9" id="14WD1prolP_" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prolPA" role="19SJt6">
                  <property role="19SUeA" value="the possible consequences of the intended further processing for data subjects;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolPB" role="3MKX6F">
              <property role="3MLT8O" value="(e)" />
              <property role="1hTQn4" value="https://calculemus.org/2ba19892-f4d3-4f7d-a081-52abd8bfba15" />
