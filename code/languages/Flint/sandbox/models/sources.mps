              <property role="3MLT8O" value="(c)" />
              <property role="1hTQn4" value="https://calculemus.org/1e576708-d4d0-4263-80e8-ff6e77127faa" />
              <node concept="19SGf9" id="14WD1prolZj" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prolZk" role="19SJt6">
                  <property role="19SUeA" value="takes all measures required pursuant to Article 32;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prolZl" role="3MKX6F">
              <property role="3MLT8O" value="(d)" />
              <property role="1hTQn4" value="https://calculemus.org/3517e5fd-402d-40a1-96fc-410cf4c0ed06" />
