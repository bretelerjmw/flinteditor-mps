              <node concept="19SUe$" id="14WD1prom7w" role="19SJt6">
                <property role="19SUeA" value="Certification bodies referred to in paragraph 1 shall be accredited in accordance with that paragraph only where they have:" />
              </node>
            </node>
          </node>
          <node concept="3MKX5h" id="14WD1prom7x" role="3MKX6F">
            <property role="1hTQn4" value="https://calculemus.org/25c5f2ee-5668-4fe1-95a9-e174f9ef3e6d" />
            <node concept="3MKX6G" id="14WD1prom7y" role="3MKX6F">
              <property role="3MLT8O" value="(a)" />
              <property role="1hTQn4" value="https://calculemus.org/3ed32d24-0213-4352-8d0e-2c42ea6dd8d5" />
              <node concept="19SGf9" id="14WD1prom7z" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prom7$" role="19SJt6">
                  <property role="19SUeA" value="demonstrated their independence and expertise in relation to the subject-matter of the certification to the satisfaction of the competent supervisory authority;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prom7_" role="3MKX6F">
              <property role="3MLT8O" value="(b)" />
              <property role="1hTQn4" value="https://calculemus.org/dec64141-bb08-40d6-8cf6-571def2d3890" />
              <node concept="19SGf9" id="14WD1prom7A" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prom7B" role="19SJt6">
                  <property role="19SUeA" value="undertaken to respect the criteria referred to in Article 42(5) and approved by the supervisory authority which is competent pursuant to Article 55 or 56 or by the Board pursuant to Article 63;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prom7C" role="3MKX6F">
              <property role="3MLT8O" value="(c)" />
              <property role="1hTQn4" value="https://calculemus.org/aaf34517-1b16-4351-93c2-03007827f270" />
              <node concept="19SGf9" id="14WD1prom7D" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prom7E" role="19SJt6">
                  <property role="19SUeA" value="established procedures for the issuing, periodic review and withdrawal of data protection certification, seals and marks;" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prom7F" role="3MKX6F">
              <property role="3MLT8O" value="(d)" />
              <property role="1hTQn4" value="https://calculemus.org/4997cd58-197f-4280-97f4-93a54dfe7c9f" />
              <node concept="19SGf9" id="14WD1prom7G" role="3MLT8Q">
                <node concept="19SUe$" id="14WD1prom7H" role="19SJt6">
                  <property role="19SUeA" value="established procedures and structures to handle complaints about infringements of the certification or the manner in which the certification has been, or is being, implemented by the controller or processor, and to make those procedures and structures transparent to data subjects and the public; and" />
                </node>
              </node>
            </node>
            <node concept="3MKX6G" id="14WD1prom7I" role="3MKX6F">
              <property role="3MLT8O" value="(e)" />
              <property role="1hTQn4" value="https://calculemus.org/f785c92e-d2e3-4066-9e49-40a67567a8d0" />
              <node concept="19SGf9" id="14WD1prom7J" role="3MLT8Q">
